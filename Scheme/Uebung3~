;------------------------------------------------------------------------------------------------------------------------------
;--------------------------------------------- Aufgabe 2 a) -------------------------------------------------------------------
;------------------------------------------------------------------------------------------------------------------------------

(define (fib n)
  (local (
          (define (fib_tr z a b)
            (cond
              ((or (= z 0) (= z 1)) b)
              (else (fib_tr (- z 1) b (+ a b))))))
     (fib_tr n 0 1)))

